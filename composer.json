{
    "name": "actengage/sluggable",
    "description": "A simple trait to ensure Laravel models have slugs.",
    "keywords": ["slugs", "sluggable"],
    "license": "MIT",
    "authors": [
        {
            "name": "Justin Kimbrell",
            "email": "jkimbrell@actengage.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "mockery/mockery": "^1.1",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Actengage\\Sluggable\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "autoload-dev": {
        "psr-4" : {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                
            ],
            "aliases": {

            }
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
